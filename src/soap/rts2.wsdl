      <element name="filter" type="xsd:double" minOccurs="1" maxOccurs="1"/>
      <element name="status" type="ns:cameraStatus" minOccurs="1" maxOccurs="1"/>
    </sequence>
  </complexType>
  <complexType name="cameras">
    <sequence>
      <element name="camera" type="ns:camera" minOccurs="0" maxOccurs="unbounded"/>
    </sequence>  
  </complexType>
  <complexType name="target">
    <sequence>
      <element name="id" type="xsd:positiveInteger" minOccurs="1" maxOccurs="1"/>
      <element name="name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
      <element name="type" type="xsd:string" minOccurs="1" maxOccurs="1"/>
      <element name="radec" type="ns:radec" minOccurs="1" maxOccurs="1"/>
      <element name="priority" type="xsd:double" minOccurs="1" maxOccurs="1"/>
    </sequence>
  </complexType>
  <complexType name="dome">
    <sequence>
      <element name="temp" type="xsd:double" minOccurs="1" maxOccurs="1"/>
      <element name="humi" type="xsd:double" minOccurs="1" maxOccurs="1"/>
      <element name="wind" type="xsd:double" minOccurs="1" maxOccurs="1"/>
