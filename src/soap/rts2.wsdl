<message name="getEquRequest">
</message>

<message name="getEquResponse">
 <part name="ra" type="xsd:double"/>
 <part name="dec" type="xsd:double"/>
</message>

<portType name="equ">
 <operation name="getEqu">
  <documentation>Get current coordinates of the telescope.</documentation>
  <input message="tns:getEquRequest"/>
  <output message="tns:getEquResponse"/>
 </operation>
</portType>

<message name="getExecRequest">
</message>

<message name="getExecResponse">
  <part name="current" type="xsd:int"/>
  <part name="next" type="xsd:int"/>
</message>

<portType name="exec">
  <operation name="getExec">
    <documentation>Get current and next target.</documentation>
    <input message="tns:getExecRequest"/>
    <output message="tns:getExecResponse"/>
  </operation>
</portType>

<message name="getDomeRequest">
</message>

<message name="getDomeResponse">
  <part name="temp" type="xsd:double"/>
  <part name="humi" type="xsd:double"/>
  <part name="wind" type="xsd:double"/>
</message>

<portType name="dome">
  <operation name="getDome">
    <documentation>Get current weather values</documentation>
    <input message="tns:getDomeRequest"/>
    <output message="tns:getDomeResponse"/>
  </operation>
</portType>

<binding name="rts2" type="tns:equ">
 <SOAP:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
 <operation name="getEqu">
  <SOAP:operation style="rpc" soapAction=""/>
  <input>
     <SOAP:body use="encoded" namespace="urn:rts2" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
  </input>
  <output>
     <SOAP:body use="encoded" namespace="urn:rts2" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
  </output>
 </operation>
</binding>

