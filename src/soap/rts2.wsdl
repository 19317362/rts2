  </complexType>
  <complexType name="filter">
    <sequence>
      <element name="filter" type="xsd:positiveInteger" minOccurs="1" maxOccurs="1"/>
    </sequence>
  </complexType>
  <simpleType name="system">
    <restriction base="xsd:string">
      <enumeration value="OFF"/>
      <enumeration value="STANDBY"/>
      <enumeration value="ON"/>
    </restriction>
  </simpleType>
  <simpleType name="daytime">
    <restriction base="xsd:string">
      <enumeration value="DAY"/>
      <enumeration value="EVENING"/>
      <enumeration value="DUSK"/>
      <enumeration value="NIGHT"/>
      <enumeration value="DAWN"/>
      <enumeration value="MORNING"/>
    </restriction>
  </simpleType>
  <simpleType name="cameraStatus">
    <restriction base="xsd:string">
      <enumeration value="IDLE"/>
      <enumeration value="IMAGE"/>
      <enumeration value="DARK"/>
      <enumeration value="READOUT"/>
      <enumeration value="ERROR"/>
    </restriction>
